<blocks app="Snap! 9.0, https://snap.berkeley.edu" version="2">
  <palette>
    <category name="Developer Tools" color="255,229,31,1"/>
  </palette>
  <block-definition s="console log %&apos;input&apos;" type="command" category="Developer Tools">
    <header></header>
    <code></code>
    <translations></translations>
    <inputs>
      <input type="%s"></input>
    </inputs>
    <script>
      <block s="doRun">
        <block s="reportJSFunction">
          <list>
            <l>input</l>
          </list>
          <l>console.log(input);</l>
        </block>
        <list>
          <block var="input"/>
        </list>
      </block>
    </script>
  </block-definition>
</blocks>